<p id="notice"><%= notice %></p>

<h1>Clock Outs</h1>

<table>
  <thead>
    <tr>
      <th>Attendance</th>
      <th>Finished at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clock_outs.each do |clock_out| %>
      <tr>
        <td><%= clock_out.attendance_id %></td>
        <td><%= clock_out.finished_at %></td>
        <td><%= link_to 'Show', clock_out %></td>
        <td><%= link_to 'Edit', edit_clock_out_path(clock_out) %></td>
        <td><%= link_to 'Destroy', clock_out, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
